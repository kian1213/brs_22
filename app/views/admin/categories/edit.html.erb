<div class="panel panel-success">
  <div class="panel-heading panel-colors">
    <h3><%= t ".title" %></h3>
  </div>
  <div class="panel-body">
    <%= form_for [:admin, @category], html: {multipart: true,
      class: "style-form form-horizontal"} do |f| %>
      <%= render "form", f: f %>
      <%= f.button :submit, class: "btn btn-success btn-block" do %>
        <i class="fa fa-lock">
          <%= t ".update" %>
        </i>
      <% end %>
    <% end %>
  </div>
</div>
